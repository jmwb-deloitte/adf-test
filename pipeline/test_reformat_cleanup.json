{
	"name": "test_reformat_cleanup",
	"properties": {
		"activities": [
			{
				"name": "Delete Test Folder",
				"type": "Delete",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ParameterisedDataset",
						"type": "DatasetReference",
						"parameters": {
							"folder": {
								"value": "@pipeline().parameters.test-folder",
								"type": "Expression"
							}
						}
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"parameters": {
			"test-folder": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Test_Reformat"
		},
		"annotations": []
	}
}