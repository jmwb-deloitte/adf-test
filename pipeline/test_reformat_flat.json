{
	"name": "test_reformat_flat",
	"properties": {
		"activities": [
			{
				"name": "Setup - Copy To Test Folder",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ParameterisedDataset",
						"type": "DatasetReference",
						"parameters": {
							"path": {
								"value": "'weatherdata/test_source/source.json'",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ParameterisedDataset",
						"type": "DatasetReference",
						"parameters": {
							"path": "'weatherdata/test/source.json'\t"
						}
					}
				]
			},
			{
				"name": "Run",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Setup - Copy To Test Folder",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "WeatherJSONReformat_DF_Testable",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"target_folder": {
							"value": "@pipeline().parameters.test_folder",
							"type": "Expression"
						},
						"target_file": {
							"value": "@pipeline().parameters.filename",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "Assert - Equal to Example",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Run",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "EqualToExample",
						"type": "DataFlowReference",
						"parameters": {
							"test-folder": {
								"value": "'@{pipeline().parameters.test_folder}'",
								"type": "Expression"
							},
							"test-file": {
								"value": "'@{pipeline().parameters.filename}'",
								"type": "Expression"
							},
							"path-to-test-folder": {
								"value": "'@{pipeline().parameters.path_to_test_folder}'",
								"type": "Expression"
							},
							"path-to-example": {
								"value": "'@{pipeline().parameters.path_to_example_folder}'",
								"type": "Expression"
							},
							"example-folder": {
								"value": "'@{pipeline().parameters.example_folder}'",
								"type": "Expression"
							},
							"example-file": {
								"value": "'@{pipeline().parameters.example_file}'",
								"type": "Expression"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			},
			{
				"name": "Cleanup - Delete Test Folder",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "Assert - Equal to Example",
						"dependencyConditions": [
							"Succeeded",
							"Failed"
						]
					},
					{
						"activity": "Setup - Copy To Test Folder",
						"dependencyConditions": [
							"Failed"
						]
					},
					{
						"activity": "Run",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ParameterisedDataset",
						"type": "DatasetReference"
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"parameters": {
			"source_path": {
				"type": "string"
			},
			"test_path": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Test_Reformat"
		},
		"annotations": []
	}
}